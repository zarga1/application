{
  "name": "angular-architecture",
  "version": "1.0.0",
  "description": "Angular Architecture",
  "license": "MIT",
  "author": "BEN TAALA OMar",
  "angular-cli": {},
  "scripts": {
    "start": "npm run sy-pre-start & \"node_modules/.bin/ng\" serve --proxy-config proxy.conf.json",
    "lint": "ng lint",
    "test": "npm run sy-pre-test & ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "sy-pre-test": "node hooks/pre-test.js",
    "sy-pre-start": "node hooks/pre-start.js",
    "sy-pre-build": "node hooks/pre-build.js",
    "sy-post-build": "node hooks/post-build.js",
    "sw": "sw-precache --root=./dist --config=sw-precache-config.js",
    "sy-build": "npm run sy-pre-build & ng build --prod --aot & npm run sy-post-build & npm run sw",
    "gen:module": "'node_modules/.bin/ng' generate module products --module app.module.ts --routing=true --spec=false",
    "gen:store": "'node_modules/.bin/ng' generate store Products --statePath products/store -m products/products.module.ts",
    "gen:feature": "'node_modules/.bin/ng' generate feature products/store/Product --reducers ../store/index.ts --spec=false"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@angular/cdk": "^6.0.0",
    "@angular/material": "^6.0.0",
    "@ngrx/effects": "^6.0.0",
    "@ngrx/entity": "^6.0.0",
    "@ngrx/router-store": "^6.0.0",
    "@ngrx/schematics": "^5.2.0",
    "@ngrx/store": "^6.0.0",
    "@ngrx/store-devtools": "^6.0.0",
    "core-js": "2.4.1",
    "ng2-translate": "^5.0.0",
    "ngrx-data": "^6.0.0-beta.6",
    "ngrx-store-freeze": "^0.2.1",
    "reselect": "^2.5.4",
    "rxjs": "^6.1.0",
    "rxjs-compat": "^6.1.0",
    "ts-helpers": "1.1.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/core": "^0.5.12",
    "@angular-devkit/build-angular": "^0.5.12",
    "@angular/cli": "^6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "fs-extra": "^1.0.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "json-concat": "0.0.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "sw-precache": "^5.0.0",
    "sw-toolbox": "^3.6.0",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "2.7.2",
    "webdriver-manager": "11.1.1"
  }
}
